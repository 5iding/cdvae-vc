{
    "model": "VAEUGAN",
    "model_module": "model.vaeugan",
    "trainer": "VAEUGANTrainer",
    "trainer_module": "trainer.vaeugan",
    "stat_dir": "/mnt/md1/datasets/vcc2018/world/etc-new",
    "spklist": "/mnt/md1/datasets/vcc2018/world/etc/speakers.tsv",

    "z_dim": 4,
	"y_dim": 12,
	"y_emb_dim": 4,
    "encoder": {
        "sp":{
            "kernel": [[7, 1], [7, 1], [7, 1], [7, 1], [7, 1]],
            "stride": [[3, 1], [3, 1], [3, 1], [3, 1], [3, 1]],
            "output": [16, 32, 64, 128, 256]
        }
	},
	"generator": {
        "sp":{
            "hwc": [19, 1, 81],
            "kernel": [[9, 1], [7, 1], [7, 1], [1025, 1]],
            "stride": [[3, 1], [3, 1], [3, 1], [1, 1]],
            "output": [32, 16, 8, 1]
        }
    },
    "discriminator": {
        "sp":{
            "kernel": [[7, 1], [7, 1], [115, 1]],
            "stride": [[3, 1], [3, 1], [3, 1]],
            "output": [16, 32, 64]
        }
	},
	"training": {
        "train_file_pattern": ["/mnt/md1/datasets/vcc2018/world/bin-dynamic/VAD/tr/*/[12]00[0-6]?.bin",
                               "/mnt/md1/datasets/vcc2018/world/bin-dynamic/VAD/tr/*/[12]0070.bin"],
        "valid_file_pattern": ["/mnt/md1/datasets/vcc2018/world/bin-dynamic/VAD/tr/*/[12]007[1-9].bin",
                               "/mnt/md1/datasets/vcc2018/world/bin-dynamic/VAD/tr/*/[12]008[0-1].bin"],
		"batch_size": 1,
		"lr": 1e-4,
		"beta1": 0.5,
        "beta2": 0.999,
        "nIterD": 5,
        "vae_iter": 1000000,
        "gan_iter": 3000000,
        "max_iter": 4000000,
        "save_freq": 1000000, 
        "summary_freq": 1000, 
        "log_freq": 1000,
        "gamma_sp": 0.5,
        "clamping": 0.01
	},
    "conversion": {
        "input": "sp",
        "output": "sp",
        "test_file_pattern": "/mnt/md1/datasets/vcc2018/world/bin-dynamic/no_VAD/ev/{}/*.bin"
    },
    "feat_param":{
        "fs": 22050,
        "shiftms": 5,
        "fftl": 1024,
        "mcep_alpha": 0.455,
        "sp_dim": 513,
        "mcc_dim": 34,
        "feat_dim": 2710,
        "dim":{
            "sp": 513,
            "mcc": 34,
            "feat": 2710
        }
    },
    "normalizer_files":{
        "sp": {
            "max": "sp_max.npf",
            "min": "sp_min.npf"
        }
    }
}