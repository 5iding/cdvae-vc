{
    "model": "VAESGAN",
    "model_module": "model.vaesgan",
    "trainer": "VAESGANTrainer",
    "trainer_module": "trainer.vaesgan",
    "stat_dir": "/mnt/md1/datasets/vcc2018/world/etc-new",
    "spklist": "/mnt/md1/datasets/vcc2018/world/etc/speakers.tsv",

    "src": "SF1",
    "trg": "TF1",

    "z_dim": 128,
	"y_dim": 12,
	"y_emb_dim": 128,
	"discriminator": {
        "sp":{
            "merge_dim": 1024,
            "kernel": [[7, 1], [7, 1], [115, 1]],
            "stride": [[3, 1], [3, 1], [3, 1]],
            "output": [16, 32, 64],
            "l2-reg": 1e-6,
            "feature_layer": 1
        }
	},
    "encoder": {
        "sp":{
            "kernel": [[7, 1], [7, 1], [7, 1], [7, 1], [7, 1]],
            "stride": [[3, 1], [3, 1], [3, 1], [3, 1], [3, 1]],
            "output": [16, 32, 64, 128, 256],
            "l2-reg": 1e-6
        }

	},
	"generator": {
        "sp":{
            "hwc": [19, 1, 81],
            "merge_dim": 171,
            "kernel": [[9, 1], [7, 1], [7, 1], [1025, 1]],
            "stride": [[3, 1], [3, 1], [3, 1], [1, 1]],
            "output": [32, 16, 8, 1],
            "l2-reg": 1e-6
        }
    },
	"training": {
        "train_file_pattern": {
            "src": ["/mnt/md1/datasets/vcc2018/world/bin-dynamic/VAD/tr/{}/[12]00[0-6]?.bin",
                    "/mnt/md1/datasets/vcc2018/world/bin-dynamic/VAD/tr/{}/[12]0070.bin"],
            "trg": ["/mnt/md1/datasets/vcc2018/world/bin-dynamic/VAD/tr/{}/[12]00[0-6]?.bin",
                    "/mnt/md1/datasets/vcc2018/world/bin-dynamic/VAD/tr/{}/[12]0070.bin"]
        },
        "valid_file_pattern": {
            "src": ["/mnt/md1/datasets/vcc2018/world/bin-dynamic/VAD/tr/{}/[12]007[1-9].bin",
                    "/mnt/md1/datasets/vcc2018/world/bin-dynamic/VAD/tr/{}/[12]008[0-1].bin"],
            "trg": ["/mnt/md1/datasets/vcc2018/world/bin-dynamic/VAD/tr/{}/[12]007[1-9].bin",
                    "/mnt/md1/datasets/vcc2018/world/bin-dynamic/VAD/tr/{}/[12]008[0-1].bin"]
        },
		"batch_size": 16,
		"lr": 1e-4,
		"beta1": 0.5,
		"beta2": 0.999,
		"gamma": 50, 
        "nIterD": 5,
		"gp_weight": 10,
		"vae_iter": 60000,
		"gan_iter": 180000,
        "max_iter": 240000,
        "save_freq": 10000, 
        "summary_freq": 1000, 
        "log_freq": 1000
	},
    "conversion": {
        "input": "sp",
        "output": "sp",
        "test_file_pattern": "/mnt/md1/datasets/vcc2018/world/bin-dynamic/no_VAD/ev/{}/*.bin"
    },
    "feat_param":{
        "fs": 22050,
        "shiftms": 5,
        "fftl": 1024,
        "mcep_alpha": 0.455,
        "sp_dim": 513,
        "mcc_dim": 34,
        "feat_dim": 2710,
        "dim":{
            "sp": 513,
            "mcc": 34,
            "feat": 2710
        }
    },
    "normalizer_files":{
        "sp": {
            "max": "sp_max.npf",
            "min": "sp_min.npf"
        }
    }
}
